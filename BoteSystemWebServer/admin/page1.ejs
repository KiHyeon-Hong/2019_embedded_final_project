              <!-- 여기부터 db로부터 받아온 것으로 체우세요오오오오 -->
              <% var arr1 = [['192.9.44.53:65001', '회원정보 및 투표관리 서버', '서버상태'],
		['192.9.44.53:65009', '유효성 검증 서버1', '서버상태'],
		['192.9.44.53:65010', '유효성 검증 서버2', '서버상태'],
		['192.9.44.53:65011', '유효성 검증 서버3', '서버상태']]
		for(var i = 0; i < 4; i++)
		{%>
		<tr style="height: 25px;">
		<%for(var j =0; j<3; j++)
		{
		if(i ==0 && j==0) { %>
		<td><b><%= url0 %></b></td>
		<%}
		else if(i ==1 && j==0) { %>
		<td><b><%= url1 %></b></td>
		<%}
		else if(i ==2 && j==0) { %>
		<td><b><%= url2 %></b></td>
		<%}
		else if(i ==3 && j==0) { %>
		<td><b><%= url3 %></b></td>
		<%}
		else if(i ==0 && j==2) { %>
		<td><b><%= status0 %></b></td>
		<%}
		else if(i ==1 && j==2) { %>
		<td><b><%= status1 %></b></td>
		<%}
		else if(i ==2 && j==2) { %>
		<td><b><%= status2 %></b></td>
		<%}
		else if(i ==3 && j==2) { %>
		<td><b><%= status3 %></b></td>
		<%}else{
		%>
		<td><b><%= arr1[i][j] %></b></td>
		<%}}%>
		</tr>
		<%}%>
  
              
  
     
  
